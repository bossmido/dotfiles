#include <stdio.h>
#include <math.h>

int main(int argc, char **argv){
    if (argc < 2){
        printf("This program takes exactly 2 arguments!");
    }
    else
    {
        int p, q, test = 1;
        printf("%d, ", 2);
        for (p = 3; p < atoi(argv[1]); p += 2){
            for (q = 3; q <= sqrt(p); q += 2){
                if (p % q == 0){
                  test = 0;
               }
            }
            if (test){
                printf("%d, ", p);
            }
            else
            {
                test = 1;
            }
        }
    }
    printf("\n");
    return 0;
}
