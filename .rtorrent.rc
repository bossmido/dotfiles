session = ./torrents
directory = ./rtorrent
schedule = watch_directory,5,5,load_start=./torrent/*.torrent
schedule = ratio,60,60,"stop_on_ratio=200,200M,2000"
max_peers = 80
max_peers_seed = 30
upload_rate = 100
download_rate = 500
port_range = 49164-49164
port_random = no
encryption = allow_incoming,try_outgoing,enable_retry
